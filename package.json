{
  "name": "light-pen",
  "version": "1.0.2",
  "description": "A variety of simple components that match an aria-role",
  "main": "./exports/index.js",
  "module": "./exports/index.js",
  "types": "./exports/index.d.ts",
  "exports": {
    ".": "./exports/index.js",
    "./package.json": "./package.json",
    "./exports/*.js": "./exports/*.js",
    "./exports/*": "./exports/*",
    "./types/*": "./types/*.d.ts"
  },
  "type": "module",
  "files": [
    "exports",
    "internal",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "web-dev-server --watch",
    "clean": "rimraf -g \"./{exports,internal}/**/*.d.ts\"",
    "build": "npm run clean && tsc && custom-elements-manifest analyze",
    "analyze": "custom-elements-manifest analyze"
  },
  "keywords": [
    "web-components",
    "aria"
  ],
  "license": "MIT",
  "repository": "git@github.com:KonnorRogers/light-pen.git",
  "bugs": {
    "url": "https://github.com/KonnorRogers/light-pen/issues"
  },
  "homepage": "https://github.com/KonnorRogers/light-pen",
  "author": "KonnorRogers <konnor5456@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.8.4",
    "rimraf": "^5.0.1",
    "typescript": "~5.2.0"
  },
  "dependencies": {},
  "customElements": "custom-elements.json"
}
